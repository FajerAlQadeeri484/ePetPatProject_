<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Company">

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="#F4E3E3"
        app:menu="@menu/bottom_navigation" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="199dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="126dp">

        <Button
            android:id="@+id/button5"
            android:layout_width="match_parent"
            android:layout_height="398dp"
            android:background="@drawable/babypink" />
    </FrameLayout>

    <TextView
        android:id="@+id/aboutus"
        android:layout_width="280dp"
        android:layout_height="73dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="136dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="194dp"
        android:background="@drawable/white"
        android:clickable="true"
        android:fontFamily="@font/josefin_sans_bold"
        android:padding="25dp"
        android:text="About us"
        android:textAlignment="center"
        android:textColor="#2F3543"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/message"
        android:layout_width="291dp"
        android:layout_height="73dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="-2dp"
        android:layout_marginEnd="122dp"
        android:layout_marginBottom="300dp"
        android:background="@drawable/white"
        android:clickable="true"
        android:fontFamily="@font/josefin_sans_bold"
        android:padding="22dp"
        android:text="           Message"
        android:textAlignment="textStart"
        android:textColor="#2F3543"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="41dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="214dp"
        android:layout_marginBottom="314dp"
        app:srcCompat="@drawable/email" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="132dp"
        android:layout_marginTop="153dp"
        android:fontFamily="@font/josefin_sans_bold"
        android:text="Grooming Service"
        android:textAlignment="center"
        android:textColor="#2F3543"
        android:textSize="18sp"
        android:textStyle="bold" />

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:background="#F4E3E3">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="61dp"
            android:layout_height="57dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="9dp"
            android:alpha="0.5"
            app:srcCompat="@drawable/thelogo" />

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="#F4E3E3"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme" />

    </com.google.android.material.appbar.AppBarLayout>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="119dp"
        android:layout_marginTop="113dp"
        android:fontFamily="@font/josefin_sans_bold"
        android:text="Company Name"
        android:textAlignment="center"
        android:textColor="#2F3543"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/call"
        android:layout_width="280dp"
        android:layout_height="73dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginEnd="-1dp"
        android:layout_marginBottom="402dp"
        android:autoLink="phone"
        android:background="@drawable/white"
        android:clickable="true"
        android:fontFamily="@font/josefin_sans_bold"
        android:padding="25dp"
        android:text="Call"
        android:textAlignment="center"
        android:textColor="#2F3543"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="41dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="162dp"
        android:layout_marginBottom="417dp"
        app:srcCompat="@drawable/phonecall" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="41dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="158dp"
        android:layout_marginBottom="212dp"
        app:srcCompat="@drawable/group" />

</RelativeLayout>